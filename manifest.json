{
  "manifest_version": 2,
  "name": "Tab Already Open",
  "version": "0.1.0",
  "description": "Notifies the user if the tab they just opened is already open",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Tab Already Open",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
